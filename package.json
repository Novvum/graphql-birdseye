{
  "name": "graphql-birdseye",
  "description": "graphql visualizer",
  "author": "novvum",
  "license": "MIT",
  "private": true,
  "repository": "novvum/graphql-birdseye",
  "keywords": [
    "graphql-visualizer",
    "graphql",
    "graphql-birdseye",
    "jointjs",
    "graphql-playground"
  ],
  "workspaces": [
    "packages/*",
    "example"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "core": "lerna run --scope 'graphql-birdseye-core' --parallel",
    "example": "lerna run --scope 'birdseye-example' --parallel",
    "dev": "lerna run --scope 'birdseye-example' --parallel develop",
    "test": "lerna run --scope 'graphql-birdseye*' --parallel test",
    "build": "lerna run --scope 'graphql-birdseye*' build",
    "build:demo": "lerna run --scope birdseye-example build",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "publish": "lerna publish"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "husky": "^1.3.1",
    "lerna": "^2.9.0",
    "prettier": "^1.16.4",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "2.9.2",
    "yarn-run-all": "3.1.1"
  },
  "files": [
    "dist"
  ]
}
