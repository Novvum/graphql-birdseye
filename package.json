{
  "name": "graphql-birdseye",
  "version": "1.1.2",
  "description": "graphql visualizer",
  "author": "novvum",
  "license": "MIT",
  "private": true,
  "repository": "novvum/graphql-birdseye",
  "keywords": [
    "graphql-visualizer",
    "graphql",
    "graphql-birdseye",
    "jointjs",
    "graphql-playground"
  ],
  "workspaces": [
    "packages/*",
    "example"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "wsrun --parallel test",
    "test:watch": "wsrun --parallel test:watch",
    "build:packages": "wsrun -p 'graphql-birdseye*' --parallel build",
    "build:demo": "yarn workspace example build",
    "build": "yarn build:packages && yarn build:demo",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "2.9.2",
    "wsrun": "^3.6.4",
    "yarn-run-all": "3.1.1"
  },
  "files": [
    "dist"
  ]
}